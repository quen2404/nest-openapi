{
  "name": "nest-openapi",
  "version": "1.0.0",
  "description": "OpenAPI middleware for NestJS",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format": "prettier lib/**/*.ts --write",
    "precommit": "lint-staged",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qdesrame/nest-openapi.git"
  },
  "keywords": [
    "NestJS",
    "OpenAPI"
  ],
  "author": "Quentin Desram√© <quen2404@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/qdesrame/nest-openapi/issues"
  },
  "homepage": "https://github.com/qdesrame/nest-openapi#readme",
  "devDependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@types/node": "^10.12.3",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.1",
    "reflect-metadata": "^0.1.12",
    "typescript": "^3.1.6"
  },
  "peerDependencies": {
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add -f"
    ]
  },
  "dependencies": {
    "ts-simple-ast": "^18.0.0"
  }
}
