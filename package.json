{
  "name": "nest-openapi",
  "version": "1.0.0",
  "description": "OpenAPI middleware for NestJS",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format": "prettier lib/**/*.ts --write",
    "precommit": "lint-staged",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "ts-node test/test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qdesrame/nest-openapi.git"
  },
  "keywords": [
    "NestJS",
    "OpenAPI"
  ],
  "author": "Quentin Desram√© <quen2404@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/qdesrame/nest-openapi/issues"
  },
  "homepage": "https://github.com/qdesrame/nest-openapi#readme",
  "devDependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@types/camelcase": "^4.1.0",
    "@types/js-yaml": "^3.11.2",
    "@types/node": "^10.12.10",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "peerDependencies": {
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add -f"
    ]
  },
  "dependencies": {
    "camelcase": "^5.0.0",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "js-yaml": "^3.12.0",
    "reflect-metadata": "^0.1.12",
    "ts-simple-ast": "^18.1.0"
  }
}
